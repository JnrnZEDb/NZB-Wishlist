<!DOCTYPE html>
<html>
    <head>
      <title>NZB Wishlist</title>
      <%= stylesheet_link_tag   'application', media: 'all', 'data-turbolinks-track' => true %>
      <%= requirejs_include_tag 'application' %>
      <%= csrf_meta_tags %>
    </head>

    <%= body_tag do %>

        <%= render partial: "shared/navbar" %>
        <div class="container">
            <div class="row">
                <%= render partial: "shared/flash_messages" %>
            </div>
            <div class="row">
                <%= yield %>
            </div>
        </div>

    <% end %>
</html>
